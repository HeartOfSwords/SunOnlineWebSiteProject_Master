<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="com.sunonline.web.bean.CollegeVoicePlayerBean" table="college_voice_player" lazy="false">
		<!-- 每个映射文件都要有id，就是该表主键 -->
		<id name="playerNumber" column="player_number">
			<generator class="native"></generator>
		</id>
		<!-- 选手姓名 -->
		<property name="playerName" column="player_name"></property>
		<!-- 选手性别 -->
		<property name="playerGender" column="player_gender"></property>
		<!-- 选手学校-->
		<property name="playerSchool" column="player_school"></property>
		
		
		<set name="collegeVoiceVideos" table="college_voice_video" cascade="all" fetch="join">
			<key>
				<column name="ZQY_video_id"></column>
			</key>
			<one-to-many class="com.sunonline.web.bean.CollegeVoiceVideoBean"/>
		</set>
		
	</class>
</hibernate-mapping>